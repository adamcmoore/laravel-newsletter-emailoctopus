{
    "name": "adamcmoore/laravel-newsletter-emailoctopus",
    "description": "EmailOctopus Driver for Spatie Laravel Newsletter",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Moore",
            "email": "adam@acmoore.co.uk"
        }
    ],
    "keywords": [
        "laravel",
        "newsletter",
        "emailoctopus"
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "goran-popovic/email-octopus-php": "^1",
        "spatie/laravel-newsletter": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "vlucas/phpdotenv": "^5.6",
        "illuminate/collections": "^10"
    },
    "autoload": {
        "psr-4": {
            "AcMoore\\LaravelNewsletter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcMoore\\LaravelNewsletter\\Tests\\": "tests/"
        }
    }
}
